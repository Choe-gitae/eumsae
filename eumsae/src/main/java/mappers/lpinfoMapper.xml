<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="lpinfo">
	<insert id="insertLpinfo" parameterType="LpVO">
		INSERT INTO lpinfo(infono,genre,title,singer,region,content,lpdate
		<if test="jpg!=null">
		,jpg,cjpg,mp3,cmp3,jpg_size,mp3_size
		</if>
		)		
		VALUES (lp_info_seq.nextval,#{genre},#{title},#{singer},#{region},#{content},#{lpdate}
		<if test="jpg!=null">
		,#{jpg},#{cjpg},#{mp3},#{cmo3},#{jpg_size},#{mp3_size}
		</if>
		)
	</insert>
</mapper>